# Local development only — enables Actor.charge() simulation
# All events default to $1 during local testing
ACTOR_TEST_PAY_PER_EVENT=true

# Writes charged events to storage/datasets/charging_log/ for inspection
ACTOR_USE_CHARGING_LOG_DATASET=true

# Apify API token — get from https://console.apify.com/account/integrations
# The SDK uses this to auto-fetch the proxy password
APIFY_TOKEN=
